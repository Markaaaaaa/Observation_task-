using System;

class Program {
  public static void Main (string[] args) {
    int[]numbers={3,14,59};
    //            0, 1, 2( index )   length is 3
    Console.WriteLine (numbers[2]);
    string[] characters = new string[] { "楚承康", "尹文鑫", "古佳跃","hihihi" };
    Console.WriteLine(characters[3]);
    int sum=0;
    for(int i=0;i<3;i++)
    {
      sum+=numbers[i];
    }
    Console.WriteLine(sum);
  }
}
